{
  "name": "ourwed-api",
  "version": "1.0.0",
  "description": "our wedding api",
  "main": "index.js",
  "scripts": {
    "prestart": "node scripts/get-env",
    "start": "node index",
    "test": "mocha tests/{models,routes}/*.test.js --recursive --exit",
    "test:all": "mocha tests/**/*.test.js --recursive -R nyan --exit",
