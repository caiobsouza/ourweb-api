  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caiobsouza/ourwed-api/issues"
  },
  "homepage": "https://github.com/caiobsouza/ourwed-api#readme",
  "dependencies": {
    "@google-cloud/storage": "^1.7.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cors": "^2.8.4",
    "crc": "^3.5.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-jwt-authz": "^1.0.0",
    "jwks-rsa": "^1.2.1",
    "mongoose": "^5.0.13",
